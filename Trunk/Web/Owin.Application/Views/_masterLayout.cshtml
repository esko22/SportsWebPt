@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en" ng-app="swptApp">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="fragment" content="!">
    <title>SportsWebPT - @Model.title</title>
    <script type="text/javascript" src="http://www.youtube.com/player_api"></script>
    <script type="text/javascript" src="http://www.swarminteractive.com/js/vm.js"></script>
    @Html.Raw(Styles.Render("~/content/css").ToString())
    @Html.Partial("_addThisSDK.cshtml")

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="~/scripts/libs/html5shiv.js"></script>
      <script src="~/scripts/libs/respond.min.js"></script>
    <![endif]-->
    @Html.Raw(Scripts.Render(
            "~/js/pre"
    ).ToString())
</head>
<body id="body-content">

    @RenderBody()

    @Html.AntiForgeryToken()
    @Html.Raw(Scripts.Render(
            "~/js/post"
    ).ToString())
    <script>
    var SportsWebPt = {
    Config: {
    AuthRedirectPath: '@Model.authRedirectPath',
    AuthorityUrl: '@Model.authorityUrl'
    }
    };
    </script>
</body>
</html>


