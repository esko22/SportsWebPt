    <div class="view container" id="admin-equipment-panel">
            <h2> Equipment</h2>
            <hr class="examine-report-hr"/>
        <div class="row">
            <div class="col-md-7">
                <table class="table table-striped">
                    <tbody data-bind="template: { name: equipmentListTemplate, foreach: equipment}">
                    </tbody>            
                </table>
            </div>
            <div class="col-md-5 well">
                <form class="form-horizontal" data-bind="validateForm: equipmentValidationOptions" id="admin-equipment-form">
                    <div class="form-group">
                        <label class="control-label col-lg-3" for="inputCommonName">Name</label>
                        <div class="col-lg-9">
                            <input type="text" class="form-control" id="inputCommonName" name="commonName" data-bind="value: selectedEquipment.commonName" placeholder="Common Name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-lg-3" for="inputCategory">Category</label>
                        <div class="col-lg-9">
                            <select id="inputCategory" class="form-control" name="category" data-bind="options: availableCategories, value: selectedEquipment.category" >
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-lg-3" for="inputProductInformation">Information</label>
                        <div class="col-lg-9">
                            <textarea id="inputProductInformation" class="form-control" name="information" rows="5" data-bind="kendoEditor: { value: selectedEquipment.productInformation }" >
                                        </textarea>
                        </div>
                   </div>
                    <div class="form-group">
                        <label class="control-label col-lg-3" for="inputPriceRange">Price Range</label>
                        <div class="col-lg-9">
                            <input type="text" class="form-control" id="inputPriceRange" name="priceRange" data-bind="value: selectedEquipment.priceRange" placeholder="Price Range">
                        </div>
                   </div>
                    <div class="text-center">
                      <button type="submit" class="btn btn-primary">Save changes</button>
                    </div>
                 </form>
            </div>
            </div>
    </div>
