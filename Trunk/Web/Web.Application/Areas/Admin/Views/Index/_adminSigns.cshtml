    <div class="view container" id="admin-sign-panel">
            <h2> Signs</h2>
            <hr class="examine-report-hr"/>
            <div class="row">
                <div class="col-md-7">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th>Description</th>
                                <th>Category</th>
                                <th>Filter</th>
                            </tr>
                        </thead>
                        <tbody data-bind="foreach: signs">
                            <tr>
                                <td data-bind="text:description"></td>
                                <td data-bind="text:category"></td>
                                <td data-bind="text:filter().filterCategory"></td>
                                <td><button class="btn-primary" data-bind="click: $parent.bindSelectedSign">Edit</button></td>
                            </tr>
                        </tbody>            
                    </table>
                </div>
                <div class="col-md-5 well">
                    <form class="form-horizontal" data-bind="validateForm: signValidationOptions" id="admin-sign-form">
                        <div class="form-group">
                            <label class="control-label col-lg-3" for="inputDescription">Description</label>
                            <div class="col-lg-9">
                                <input type="text" id="inputDescription" class="form-control" name="description" data-bind="value: selectedSign.description" placeholder="Description">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-lg-3" for="inputCategory">Category</label>
                            <div class="col-lg-9">
                                <select id="inputCategory" name="category" class="form-control" data-bind="options: availableCategories, value: selectedSign.category" >
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-lg-3" for="inputCategory">Filter</label>
                            <div class="col-lg-9">
                                <select id="inputGroup" name="group" class="form-control" data-bind="options: availableSignFilters, optionsText: 'filterCategory', optionsValue: 'id', value: selectedSign.filterId" >
                                </select>
                            </div>
                       </div>

                        <div class="text-center">
                          <button type="submit" class="btn btn-primary">Save changes</button>
                        </div>
                     </form>
                </div>
            </div>
    </div>