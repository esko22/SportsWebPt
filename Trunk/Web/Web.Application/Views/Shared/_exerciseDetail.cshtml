<div class="row">
    <div class="col-md-6 pull-left">
        <h2 data-bind="text: name"></h2>
    </div>
    <div class="col-md-offset-4 col-md-2">
        <!-- ko with: $parent.shareBar -->
        @{Html.RenderPartial("~/Views/Shared/_shareBar.cshtml"); }
        <!-- /ko --> 
    </div>
</div> 
<hr class="examine-report-hr"/>
    <div class="row">
    <div class="col-md-10">
        <label>Description: </label><span data-bind="html: description"></span>
    </div>
</div>
<div class="row">
    <div class="col-md-5">
        <label><strong>Duration: </strong><span data-bind="text: duration"></span> minutes</label>
    </div>
    <div class="col-md-5">
        <label><strong>Difficulty: </strong><span data-bind="text: difficulty"></span></label>
    </div>
</div>
<div class="row">
    <div class="col-md-2">
        <h3>Equipment</h3>
        <table class="table table-striped">
            <tbody data-bind="foreach: equipment">
                <tr>
                    <td data-bind="text: commonName"></td>
                </tr>
            </tbody>            
        </table>
    </div>
    <div class="col-md-7">
        <h3>Video</h3>
        <!-- ko with: videos()[0] -->
        <div class="video-container">
            <iframe data-bind="attr: { src:'http://www.youtube.com/embed/' + youtubeVideoId()  } " id="ytExercisePlayer" width="560" height="315" frameborder="0"></iframe>
        </div>
        <!-- /ko -->

    </div>
</div>    
