<div id="research-exercise-panel">
    <span aria-hidden="true" data-icon="&#xe007;" class="research-header-icon"> Exercises</span>
    <hr class="examine-report-hr"/>
    <div class="row" data-bind="mask: !isInitialized()">
        <div class="col-md-3 research-vertical-splitter" >
            <h3>Body Regions</h3>
            <a class="label-filter-hover"><span class="label label-danger" data-bind="click: onReset">Show All</span></a>
            <ul class="list-no-style filter-container" data-bind="foreach: bodyRegions">
                <li class="filter-label">
                    <a><span class="label label-info" data-bind="text: name, click: $parent.onFilterSelect"></span></a>
                </li>
            </ul>
        </div>
        <div class="col-md-9">
            <div class="panel-group" id="recover-exercise-accordion">
                <section data-bind="template: { name: briefExerciseTemplate, foreach: filteredExercises }"></section>
            </div>
        </div>
    </div>
</div>
