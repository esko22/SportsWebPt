<div class="container" style="display: none" data-bind="visible: isVisible" id="examine-content">
    <div>
        <span class="header-icon"><i class="icon-sportswebpt-stethoscope"></i> Examine</span> 
    </div>
    <div>
        <ul class="breadcrumb main-breadcrumb" id="examine-nav">
            <li><a href="#/examine" id="skeleton-nav">Select Region(s)</a></li>
            <li><a href="#/examine/detail" id="detail-nav">Describe Discomfort</a></li>
            <li><a id="report-nav" style="text-decoration: none; cursor: default;">Potential Injuries</a></li>
        </ul>
    </div>
    
    
    
    

    <!-- ko with: skeleton -->
        <div class="view" id="examine-skeleton-pane">
            @{Html.RenderPartial("~/views/examine/_examineSkeleton.cshtml");}
        </div>
        <!-- /ko -->
    l
        <!-- ko with: detail -->
        <div class="view" id="examine-detail-pane">
            @{Html.RenderPartial("~/views/examine/_examineSymptomDetail.cshtml");}
        </div>
        <!-- /ko -->

        <!-- ko with: report -->
        <div class="view" id="examine-report-pane">
            @{Html.RenderPartial("~/views/examine/_examineInjuryReport.cshtml");}
        </div>
        <!-- /ko -->
        
    
        <!-- Modal -->
        <div class="modal fade" id="examine-report-modal" tabindex="-1" role="dialog" aria-hidden="true">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <h4 class="modal-title">SportsWebPt</h4>
              </div>
                <div class="modal-body">
                    <h4>Comparing your response against our database...</h4>                       
                    <div class="progress progress-striped active push-down-md">
                        <div class="progress-bar progress-bar-info"  role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                            <span class="sr-only">100% Complete</span>
                        </div>
                    </div>
                </div>
              <div class="modal-footer">
                  <h5 class="text-center">
                        SportsWebPT exercises are designed by physical therapists in order to promote an active and pain-free lifestyle!
                  </h5>
              </div>
            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->
</div>
